{
  "name": "desmoines-ai-pulse-mobile",
  "version": "1.0.0",
  "description": "Native mobile app for Des Moines AI Pulse - iOS & Android",
  "private": true,
  "scripts": {
    "dev": "vite --config vite.config.mobile.ts",
    "build": "vite build --config vite.config.mobile.ts",
    "build:ios": "npm run build && cap sync ios",
    "build:android": "npm run build && cap sync android",
    "build:all": "npm run build && cap sync",
    "preview": "vite preview --config vite.config.mobile.ts --port 8081",
    "sync": "cap sync",
    "sync:ios": "cap sync ios",
    "sync:android": "cap sync android",
    "open:ios": "cap open ios",
    "open:android": "cap open android",
    "add:ios": "cap add ios",
    "add:android": "cap add android",
    "update": "cap update",
    "run:ios": "cap run ios",
    "run:android": "cap run android",
    "lint": "cd .. && npm run lint",
    "type-check": "cd .. && npm run type-check"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "^6.0.1",
    "@capacitor/browser": "^6.0.2",
    "@capacitor/camera": "^6.0.2",
    "@capacitor/core": "^6.2.0",
    "@capacitor/geolocation": "^6.0.1",
    "@capacitor/haptics": "^6.0.1",
    "@capacitor/ios": "^6.2.0",
    "@capacitor/keyboard": "^6.0.2",
    "@capacitor/local-notifications": "^6.0.1",
    "@capacitor/network": "^6.0.2",
    "@capacitor/preferences": "^6.0.2",
    "@capacitor/push-notifications": "^6.0.2",
    "@capacitor/share": "^6.0.2",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "capacitor-native-biometric": "^4.2.2",
    "capacitor-plugin-app-tracking-transparency": "^2.0.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
